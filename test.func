defn reverse l = {
    case l of {
        [] -> {[]}
        [x: xs] -> {(reverse xs) ++ [x]}
    }
}
defn cmp a b = {
    case a of {
        [] -> {True}
        [x: xs] -> {
            case b of {
                [] -> {False}
                [y: ys] -> { (charToNum x == charToNum y) && (cmp xs ys) }
            }
        }
    }
}
defn inputStr = {"1"}
defn main = { cmp inputStr (reverse inputStr) }